<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <changeSet id="load-lookup-data" author="navadeep">
        <loadData
                file="data/lookups.csv"
                tableName="lookups"
                separator=",">
            <column name="lookup_category" type="STRING"/>
            <column name="lookup_code" type="STRING"/>
            <column name="lookup_name" type="STRING"/>
        </loadData>
    </changeSet>

</databaseChangeLog>
